{{! Це код container файлу для summary сторінки, 
в якому міститься код, котрий буде віддаватися по  шляху (PATH) - "/summary" }}

<div>
  {{#with person}}
    <div>
      <h1 class="mb-5">{{name}}</h1>

      <p>Age: {{age}}</p>
      <p>Gender: {{gender}}</p>
    </div>

    {{#with address}}
      <div>
        <p><address>Street: {{street}}</address></p>
        <p><address>City: {{city}}</address></p>
        <p><address>State: {{state}}</address></p>
        <p>Zip: {{zip}}</p>
        <p><address>Country: {{country}}</address></p>
      </div>
    {{/with}}

    <ul class="ps-5 pt-4">
      {{#each education}}
        <div>
          <li>Degree: {{degree}}</li>
          <li>Major: {{major}}</li>
          <li><em>University: {{university}}</em></li>
          <li><time>Year: {{graduationYear}}</time></li>
        </div>
      {{/each}}
    </ul>

    <ul class="ps-5 pt-4">
      {{#each workExperience}}
        <div>
          <li class="mb-4"><em>Company: {{company}}</em></p></li>

          <li class="my-5">
            <h2><em>{{title}}</em></h2>
          </li>

          <li><time>Start date: {{startDate}}</time></li>
          <li><time>End date: {{endDate}}</time></li>
        </div>
      {{/each}}

      <ul class="ps-5 pt-4">
        {{#each workExperience}}
          {{#each responsibilities}}
            <div>
              <li><q>{{this}}</q></li>
            </div>
          {{/each}}
        {{/each}}
      </ul>
    </ul>

    <div class="my-4">
      {{#each workExperience}}
        <div>
          <p><time>Year founded: {{year_founded}}</time></p>
          <p><i>Industry: {{industry}}</i></p>
        </div>
      {{/each}}
    </div>

    <ul class="ps-5">
      {{#each workExperience}}
        {{#each employees}}
          <div>
            <li>Name: {{name}}</li>

            <li class="my-5"><h2><em>{{position}}</em></h2></li>

            <li><em>Department: {{department}}</em></li>
          </div>
        {{/each}}
      {{/each}}
    </ul>

    <ul class="ps-5 pt-4">
      {{#each workExperience}}
        {{#each employees}}
          {{#each projects}}
            <div>
              <li>Name: {{name}}</li>
              <li>Description: <q>{{description}}</q></li>
              <li>Status: {{status}}</li>
            </div>
          {{/each}}
        {{/each}}
      {{/each}}
    </ul>

    <ul class="ps-5 pt-4">
      {{#each workExperience}}
        {{#each employees}}
          {{#each projects}}
            {{#each teams}}
              <div>
                <li class="mb-4">Team name: {{team_name}}</li>

                {{#with team_leader}}
                  <div>
                    <p>Name: {{name}}</p>

                    <li class="my-5"><h3><em>{{title}}</em></h3></li>

                    <p>Email:
                      <a href={{email}}>{{email}}</a></p>
                  </div>
                {{/with}}

                <ul class="ps-5 pt-4">
                  {{#each team_members}}
                    <div>
                      <li>Name: {{name}}</li>

                      <li class="my-5"><h3><em>{{title}}</em></h3></li>

                      <li>Email: <a href={{email}}>{{email}}</a></li>
                    </div>
                  {{/each}}
                </ul>

                <ul class="ps-5 pt-4">
                  {{#each team_members}}
                    {{#each skills}}
                      <div>
                        <li>{{this}}</li>
                      </div>
                    {{/each}}
                  {{/each}}
                </ul>

                <ul class="ps-5 pt-4">
                  {{#each team_members}}
                    {{#each projects}}
                      <div>
                        <li>Name: {{name}}</li>
                        <li>Description: <q>{{description}}</q></li>
                      </div>
                    {{/each}}
                  {{/each}}
                </ul>

                <ul class="ps-5 pt-4">
                  {{#each team_members}}
                    {{#each projects}}
                      {{#each technologies}}
                        <div>
                          <li>{{this}}</li>
                        </div>
                      {{/each}}
                    {{/each}}
                  {{/each}}
                </ul>

                <ul class="ps-5 pt-4">
                  {{#each team_members}}
                    {{#each projects}}
                      {{#each team_members}}
                        <div>
                          <li>Name: {{name}}</li>

                          <li class="my-5"><h4><em>{{title}}</em></h4></li>

                        </div>
                      {{/each}}
                    {{/each}}
                  {{/each}}
                </ul>

              </div>
            {{/each}}
          {{/each}}
        {{/each}}
      {{/each}}
    </ul>
  {{/with}}
</div>

{{!-- Цей код містить лише те, що буде виводитись в {{{body}}} в layout/default.hbs 
Адже наш layout/default.hbs має вже структуру HTML --}}